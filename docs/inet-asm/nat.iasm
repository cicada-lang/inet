@zero/in 0
@zero/out 1
@zero/node :value!

@add1/in 1
@add1/out 1
@add1/node :prev :value!

@add/in 2
@add/out 1
@add/node :target! :addend :result

@add/-/zero
  $result $addend
  addend result connect END

@add/-/add1
  $prev
  $result $addend
  prev addend add add1
  result connect END

@two zero add1 add1 END

@main
  two two add
  wire-print-net
  run
  wire-print-net
  END
